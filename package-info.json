{
  "name": "echobooster-full-package",
  "version": "4.1.0",
  "description": "Complete EchoBooster v4.1 Package with all startup scripts and documentation",
  "scripts": {
    "start": "node server.js",
    "start-app": "concurrently \"npm run start\" \"npm run run-csharp\"",
    "run-csharp": "dotnet run --project EchoBooster/EchoBooster.csproj",
    "dev": "nodemon server.js",
    "build": "dotnet build EchoBooster/EchoBooster.csproj",
    "run": "npm run build && dotnet run --project EchoBooster/EchoBooster.csproj",
    "install-all": "npm install && dotnet restore",
    "clean": "rm -rf EchoBooster/bin EchoBooster/obj 2>/dev/null || true",
    "full-start": "npm run clean && npm run install-all && npm run start-app"
  },
  "keywords": [
    "system",
    "optimizer",
    "performance",
    "monitoring",
    "nodejs",
    "csharp",
    "windows",
    "process",
    "management",
    "advanced",
    "optimization"
  ],
  "author": "EchoBooster Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "systeminformation": "^5.18.6",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "ws": "^8.13.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/echobooster/echobooster.git"
  },
  "bugs": {
    "url": "https://github.com/echobooster/echobooster/issues"
  },
  "homepage": "https://github.com/echobooster/echobooster#readme"
}